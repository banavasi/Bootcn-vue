{
  "name": "@bootcn-vue/tooltip",
  "version": "0.1.0",
  "description": "Accessible, customizable tooltip component built with Bootstrap 5 and Vue 3",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
